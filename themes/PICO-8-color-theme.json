{
  "name": "PICO-8",
  "type": "dark",
  "colors": {
    "button.background": "#008751",
    "button.foreground": "#fff1e8",
    "button.hoverBackground": "#00e436",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#fff1e8",
    "activityBar.dropBackground": "#5f574f",
    "activityBar.inactiveForeground": "#c2c3c7",
    "activityBarBadge.background": "#29adff",
    "activityBarBadge.foreground": "#fff1e8",

    "sideBar.background": "#000000",
    "sideBar.foreground": "#fff1e8",
    "sideBar.dropBackground": "#c2c3c766",
    "sideBar.border": "#000000",
    "sideBarTitle.foreground": "#c2c3c7",
    "sideBarSectionHeader.background": "#5f574f",
    "sideBarSectionHeader.foreground": "#fff1e8",

    "editorGroup.border": "#000000",
    "editorGroup.dropBackground": "#5f574f66",
    "editorGroupHeader.noTabsBackground": "#ff004d",
    "editorGroupHeader.tabsBackground": "#ff004d",
    "editorGroupHeader.tabsBorder": "#ff000000",
    "editorGroup.emptyBackground": "#5f574f",
    "editorGroup.focusedEmptyBorder": "#c2c3c7",
    "tab.activeBackground": "#fff1e8",
    "tab.activeForeground": "#ff004d",
    "tab.border": "#ff004d00",
    "tab.inactiveBackground": "#ff77a8",
    "tab.inactiveForeground": "#ff004d",
    "editorPane.background": "#000",

    "editor.background": "#5f574f",
    "editor.foreground": "#c2c3c7",
    "editorLineNumber.foreground": "#83769c",
    "editorLineNumber.activeForeground": "#c2c3c7",
    "editorCursor.foreground": "#ff004d",
    "editor.selectionBackground": "#ffec27",
    "editor.lineHighlightBorder": "#ff000000",

    "editorWidget.background": "#000000",
    "editorWidget.border": "#5f574f",

    "peekViewEditor.background": "#83769c",

    "panel.background": "#000000",
    "panel.border": "#5f574f",

    "titleBar.activeBackground": "#008751",
    "statusBar.background": "#ff004d",
    "statusBar.foreground": "#7e2553"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#4bb1b1"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#83769c"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#fff1e8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff004d"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#fff1e8"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#ff77a8"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#fff1e8"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff004d"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "Support Functions",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#29adff"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#29adff"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ff77a8"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#ff77a8"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#c2c3c7"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "foreground": "#29adff"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffa300"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ffa300"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ffa300"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#ffa300"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff004d"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#7e2553"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#00e436"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
